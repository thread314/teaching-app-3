# Delete existing data
Level.destroy_all
Cardstate.destroy_all
User.destroy_all
Card.destroy_all

# Create new user
a = User.create(name: "Arthur")
userid = a.id

# Create cards
cardnum = 1

while cardnum < 21
	if cardnum < 11
		successesnum = -2
		lastsuccessnum = ""
	else
		successesnum = rand(4)
		lastsuccessnum = Date.today - rand(9)
	end
	newcard = Card.create
	newcard.levels.create(question: "What is 1 x #{cardnum}? {#{cardnum}}")
	newcard.cardstates.create(successes: successesnum, lastsuccess: lastsuccessnum, user_id: userid)
	cardnum += 1
  sleep(1)
end
